{
    "title": "Publisher Schema",
    "type": "object",
    "required": [
        "name",
        "title"
    ],
    "properties": {
        "name": {
            "description": "The short name of the publisher used in URLs",
            "type": "string",
            "unique_publisher": true
        },
        "title": {
            "description": "The full title of the publisher, e.g Cabinet Office",
            "type": "string"
        },
        "url": {
            "description": "Home page for the publisher",
            "type": "string"
        }
    }
}
